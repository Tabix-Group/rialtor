{
  "name": "remax-backend",
  "version": "1.0.0",
  "description": "Backend API for RE/MAX Knowledge Platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node scripts/start-resilient.js",
    "start:basic": "npx prisma migrate deploy --schema=./prisma/schema.prisma && npm run db:seed && node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run migrate:deploy",
    "migrate:dev": "npx prisma migrate dev --schema=./prisma/schema.prisma",
    "migrate:deploy": "npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=./prisma/schema.prisma",
    "db:seed": "node prisma/seed.js",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.24.1",
    "pdf-parse": "^1.1.1",
    "prisma": "^5.7.1",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.33.1",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "remax",
    "real-estate",
    "knowledge-base",
    "ai-bot",
    "argentina"
  ],
  "author": "RE/MAX Argentina",
  "license": "PRIVATE",
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}